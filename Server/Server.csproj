    <PackageReference Include="MudBlazor" Version="6.10.0" />
    <PackageReference Include="Stl.Fusion.Blazor" Version="6.5.1" />
    <PackageReference Include="Stl.Fusion.Blazor.Authentication" Version="6.5.1" />
